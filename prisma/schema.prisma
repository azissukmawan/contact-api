generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Contact {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  company   String?
  message   String   @db.Text
  createdAt DateTime @default(now())
}
